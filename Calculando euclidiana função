#include <stdio.h>
#include <math.h>

void calcular_euclidiana()
{
    int x1, x2, y1, y2, distancia;
    
    printf("Digite um valor x1: ");
    scanf("%d", &x1);
    
    printf("Digite um valor y1: ");
    scanf("%d", &y1);
    
    printf("Digite um valor x2: ");
    scanf("%d", &x2);
    
    printf("Digite um valor y2: ");
    scanf("%d", &y2);
    
    distancia = sqrt((pow(x2 - x1)) + (pow(y2 - y1));
    
    printf("%d", distancia);
}

int main()
{
    puts("Funcione maquina: ");
    
    calcular_euclidiana();
    
}
