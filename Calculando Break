#include <stdio.h>

int main()
{
    int n, s;
    
    do{
    printf("Qual o tamanho do vetor? ");
    scanf("%d", &n);
    if (n > 10)
        printf("NÃƒOOOOOOOOOO");
    }while (n>10);
    
    
    int v[n];

    for(int i = 0; i < n; i++){
        printf("Quais serao os numeros do vetor");
        scanf("%d", &v[i]);
    }
    
    bool sim = true;
    
    for(int i = 0; i < n-1; i++){
        s = v[i+1] / v[i];
        if(s != 2)
        sim = false;
        if (sim == false)
        break;
    }
    
    if(sim == true)
        printf("SIM");
    else 
        printf("NOPE");
    
    
    return 0;
}
